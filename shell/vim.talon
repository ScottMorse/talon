app: alacritty
app: code

-

[vim] new line [down]:
    key(escape)
    key(o)
    
[vim] new line up:
    key(escape)
    key(O)

[vim] [go] [line] <digits>:
    key(escape)
    insert(digits)
    key(G)

[vim] [go] (top | way up):
    key(escape)
    key(g:2)

[vim] insert:
    key(escape)
    key(i)

[vim] [go] (bottom | end | way down):
    key(escape)
    key(i)

[vim] visual:
    key(escape)
    key(v)

[vim] replace:
    key(escape)
    ":%s/"

[vim] append:
    key(escape)
    key(a)