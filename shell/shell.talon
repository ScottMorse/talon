app: alacritty

-

[shell] (cd | go): "cd "
[shell] (ls | list): "ls "

[shell] (search | AG): "ag "
[shell] (search files | AG files): "ag -g "

[shell] (control c | cancel): key(ctrl-c)
[shell] (copy): key(ctrl-shift-c)
[shell] (paste): key(ctrl-shift-v)

[shell] (RM | remove): "rm "
[shell] (RM | remove) force: "rm -rf "

[shell] updir: "../"
[shell] currdir: "./"